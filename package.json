{
    "name": "playwright_ui_tests_examples",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "lint": "eslint . --ext .ts,.js",
        "lint:fix": "eslint . --ext .ts,.js --fix",
        "format": "prettier --write \"**/*.{js,ts,json,md}\"",
        "format:check": "prettier --check \"**/*.{js,ts,json,md}\"",
        "test:prod:headed": "cross-env ENVIRONMENT=prod npx playwright test --project=chromium --headed",
        "test:dev:headed": "cross-env ENVIRONMENT=dev npx playwright test --project=chromium --headed",
        "test:prod:headless": "cross-env ENVIRONMENT=prod npx playwright test --project=chromium",
        "test:dev:headless": "cross-env ENVIRONMENT=dev npx playwright test --project=chromium",
        "test:prod:headless:report": "cross-env ENVIRONMENT=prod npx playwright test --project=chromium && allure generate allure-results --clean -o allure-report && allure open allure-report",
        "test:dev:headless:report": "cross-env ENVIRONMENT=dev npx playwright test --project=chromium && allure generate allure-results --clean -o allure-report && allure open allure-report"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mpisaryk/Playwright_UI_tests_examples.git"
    },
    "keywords": [],
    "author": "Mikhail Pisaryk",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/mpisaryk/Playwright_UI_tests_examples/issues"
    },
    "homepage": "https://github.com/mpisaryk/Playwright_UI_tests_examples#readme",
    "description": "",
    "devDependencies": {
        "@faker-js/faker": "^10.0.0",
        "@playwright/test": "^1.55.1",
        "@types/dotenv": "^6.1.1",
        "@types/node": "^24.5.2",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "@typescript-eslint/parser": "^8.44.0",
        "allure-commandline": "^2.34.1",
        "allure-playwright": "^3.4.1",
        "cross-env": "^10.0.0",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-playwright": "^2.2.2",
        "prettier": "^3.6.2"
    },
    "dependencies": {
        "dotenv": "^17.2.2"
    }
}
